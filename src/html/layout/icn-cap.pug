mixin icnCap (icn, caption)
    <table cellpadding="0" cellspacing="0" style="width:100%; border-collapse:collapse; border-spacing:0; margin:0; border:0;">
    <tbody>
    <tr>
    <td style="padding-left:15px; padding-right:15px;">
    <table align="center" class="r" style="margin: 0 auto;background-color:#ffffff;border-spacing: 0;width:640px;">
    <tbody>
    <tr>
    <td style="padding-top:40px;padding-bottom:20px;padding-left:30px;padding-right:30px;">
    <table border="0" cellpadding="0" cellspacing="0" style="table-layout: fixed;" width="100%">
    <tbody>
    <tr>
    <td style="width: 56px;" valign="middle"><img src="#{icn}" style="display: block; width: 100%;" width="56">
    </td>
    <td style="padding-left: 20px;" valign="middle">
    <table border="0" cellpadding="0" cellspacing="0" width="100%;">
    <tbody>
    <tr>
    <td style="text-align: left;">
    <div style="font-weight: normal; font-family: Fira Sans, sans-serif; color:#222222;font-size:25px;">
    <strong>#{caption}</strong><br>
    </div>
    </td>
    </tr>
    </tbody>
    </table>
    </td>
    </tr>
    </tbody>
    </table>
    </td>
    </tr>
    </tbody>
    </table>
    </td>
    </tr>
    </tbody>
    </table>